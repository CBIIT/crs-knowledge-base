        <client_script><![CDATA[function($scope, $location) {
  var c = this;
  var s = $location.search();
  var redirect = false;

  if (s.xpage && s.xpage == 'kd_search') {
    c.xpage = "main";
  }
	else if(s.xpage && s.xpage == 'kd_faqs'){
		c.xpage = "faqs";
	}
	else if(s.xpage && s.xpage == 'kd_glossary'){
		c.xpage = "glossary";
	}
	else if(s.xpage && s.xpage == 'kd_article'){
		c.xpage = "article";
		c.cat = s.cat;
	}
	else{
		c.xpage = "home";
	}
	
	
	
	//Search on enter key pressed
	c.inputOnkeypress = function(){
		if (event.keyCode === 13) {
			c.searchKB();
		}
	}
	
	c.showAllContent = function(){
		c.xpage = "main";
		s.xpage = 'kd_search'; // this is our default page
    $location.search(s).replace();
	}
	
	c.searchKB = function(){
		c.xpage = "main";
		s.xpage = 'kd_search';
		s.search = c.data.searchString;
    $location.search(s).replace();
	}
	
}]]></client_script>
