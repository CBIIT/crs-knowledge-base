        <operation_script><![CDATA[(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {

    var number = request.body.data.number;
	var reference = request.body.data.reference;
	var template = request.body.data.template;
	
	var link = new GlideRecord('x_26385_crs_kd_linked_records');
	link.initialize(); 
	link.article_number = number; 
	link.reference_article = reference;
	link.category = template;
	var id = link.insert();
	
	return id;

})(request, response);]]></operation_script>
