        <name>getUserGuide</name>
        <operation_script><![CDATA[(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {

	var attachmentId = "";
	var document_id = "b149e3c0dbaabf00b86770c08c9619a1";
    //var document_id = "975fe97b4f9213007221e321a310c75d";
	var gr = new GlideRecord('sys_attachment');
	gr.addQuery("table_sys_id", document_id);
	gr.addQuery('content_type','STARTSWITH','application/pdf');
	gr.query();
	while (gr.next()){
		attachmentId = gr.sys_id.toString();
		break;
	}

	response.setStatus(200);
	response.setHeaders({
		'Content-Type': gr.getValue('content_type')
	});
	
	var stream = new GlideSysAttachment().getContentStream(attachmentId);
	response.getStreamWriter().writeStream(stream);

})(request, response);]]></operation_script>
